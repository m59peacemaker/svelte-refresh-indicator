{
  "name": "svelte-refresh-indicator",
  "version": "0.0.0",
  "description": "Material design refresh indicator.",
  "main": "build/Indicator.js",
  "svelte": "src/Indicator.html",
  "svelte.root": "src",
  "scripts": {
    "build": "npm run build:main && npm run build:docs",
    "prebuild:main": "mkdir -p ./build",
    "build:main": "browserify -t sveltify src/Indicator.html > build/Indicator.js",
    "build:docs": "browserify src/docs.js > docs/docs.js"
  },
  "keywords": [
    "material",
    "spinner",
    "arrow",
    "progress",
    "pull",
    "refresh",
    "indicator",
    "css",
    "android",
    "google"
  ],
  "author": "Johnny Hauser <johnnyhauser@gmail.com>",
  "license": "CC0-1.0",
  "dependencies": {},
  "devDependencies": {
    "browserify": "14.4.0",
    "sveltify": "^1.1.0"
  }
}
